{
  "name": "pancake-info-api",
  "version": "1.0.0",
  "description": "Serverless API implementation for PancakeSwap on-chain data query.",
  "main": "dist/index.js",
  "repository": "git@github.com:pancakeswap/pancake-info-api.git",
  "author": "PancakeSwap",
  "license": "MIT",
  "scripts": {
    "format:check": "prettier --check '*/**/*.{js,ts}'",
    "format:write": "prettier --write '*/**/*.{js,ts}'",
    "lint": "eslint '*/**/*.{js,ts}'",
    "codegen": "graphql-codegen --config utils/apollo/codegen.yml",
    "build": "rimraf dist && tsc",
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -q dist/index.js\"",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "prestart": "yarn build"
  },
  "dependencies": {
    "@apollo/client": "^3.3.13",
    "@ethersproject/address": "^5.1.0",
    "bignumber.js": "^9.0.1",
    "cross-fetch": "^3.1.3",
    "graphql-tag": "^2.11.0",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.22",
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@graphql-codegen/cli": "^1.21.3",
    "@graphql-codegen/typescript": "^1.17.11",
    "@graphql-codegen/typescript-operations": "^1.17.11",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "@vercel/node": "^1.9.1",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "graphql": "^15.5.0",
    "husky": "^4.3.8",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7"
  }
}
